EXEC (FCREATE "Vehicle" "ROOT" Generic)
EXEC (FCREATE "Car" "Vehicle" Generic)
EXEC (FPUT "Car" "country" (PutN [EVAL (OBJ (S "unknown country"))]))
EXEC (FCREATE "Truck" "Vehicle" Generic)
EXEC (FCREATE "JapaneseCar" "Car" Generic)
EXEC (FCREATE "GermanCar" "Car" Generic)
EXEC (FCREATE "Toyota" "JapaneseCar" Individual)
EXEC (FCREATE "BMW" "GermanCar" Individual)
EXEC (FCREATE "Logan" "GermanCar" Individual)
EXEC (FCREATE "Roman" "Truck" Individual)
EXEC (FPUT "Vehicle" "country" (PutD (S "?")))
EXEC (FPUT "JapaneseCar" "country" (PutV (S "Japan")))
EXEC (FPUT "GermanCar" "country" (PutV (S "Germany")))
EXEC (FPUT "Roman" "country" (PutV (S "Romania")))
EXEC (FPUT "Logan" "country" (PutV (S "Romania")))
EXEC (FPUT "Logan" "rom_price" (PutV (R 42)))
EXEC (FPUT "Roman" "rom_price" (PutV (R 52)))
EXEC (FPUT "Vehicle" "fuelConsumption" (PutV (R 4.2)))
EVAL (DOT "Car" "country")

